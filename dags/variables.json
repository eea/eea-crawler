{
    "Sites": {
        "bise": "config_bise",
        "climate": "config_climate",
        "eea": "config_eea",
        "eionet": "config_eionet",
        "energy": "config_energy",
        "fise": "config_fise",
        "industry": "config_industry",
        "wise_freshwater": "config_wise_freshwater",
        "wise_marine": "config_wise_marine"
    },
    "config_bise": {
        "concurrency": 2,
        "nlp_preprocessing": {
            "text": {
                "split_length": 500
            }
        },
        "normalizers_variable": "default_normalizers",
        "scrape_pages": true,
        "scrape_with_js": true,
        "url": "https://biodiversity.europa.eu",
        "url_api_part": "api"
    },
    "config_climate": {
        "avoid_cache_api": true,
        "avoid_cache_web": true,
        "concurrency": 4,
        "nlp_preprocessing": {
            "text": {
                "split_length": 500
            }
        },
        "normalizers_variable": "default_normalizers",
        "scrape_pages": false,
        "scrape_with_js": true,
        "url": "https://climate-adapt.eea.europa.eu",
        "url_api_part": ""
    },
    "config_eea": {
        "nlp_preprocessing": {
            "text": {
                "blacklist": [
                    "contact",
                    "rights"
                ],
                "props": [
                    "description",
                    "key_message",
                    "summary",
                    "text"
                ],
                "split_length": 500
            }
        },
        "normalizers_variable": "default_normalizers",
        "pdf_days_limit": 730,
        "portal_types": [
            "Highlight",
            "Dashboard",
            "CloudVideo",
            "GIS%20Application",
            "Article",
            "CallForInterest",
            "CallForProposal",
            "CallForTender",
            "ExternalDataSpec",
            "Data",
            "DavizVisualization",
            "Document",
            "Fiche",
            "EEAFigure",
            "File",
            "Folder",
            "Assessment",
            "AssessmentPart",
            "Infographic",
            "Report",
            "Term",
            "Organisation",
            "helpcenter_faq",
            "ims_indicator"
        ],
        "scrape_for_types": {
            "ims_indicator": {
                "scrape_with_js": true
            }
        },
        "scrape_pages": false,
        "url": "http://www.eea.europa.eu",
        "url_api_part": "api/SITE"
    },
    "config_eea_tmp": {
        "nlp_preprocessing": {
            "text": {
                "blacklist": [
                    "contact",
                    "rights"
                ],
                "props": [
                    "description",
                    "key_message",
                    "summary",
                    "text"
                ],
                "split_length": 500
            }
        },
        "normalizers_variable": "default_normalizers",
        "portal_types": [
            "Highlight"
        ],
        "scrape_pages": false,
        "url": "http://www.eea.europa.eu",
        "url_api_part": "api/SITE"
    },
    "config_eionet": {
        "avoid_cache_api": true,
        "avoid_cache_web": true,
        "concurrency": 1,
        "nlp_preprocessing": {
            "text": {
                "split_length": 500
            }
        },
        "normalizers_variable": "default_normalizers",
        "scrape_pages": false,
        "scrape_with_js": true,
        "types_blacklist": [
            "Event"
        ],
        "url": "https://www.eionet.europa.eu",
        "url_api_part": ""
    },
    "config_energy": {
        "nlp_preprocessing": {
            "text": {
                "split_length": 500
            }
        },
        "normalizers_variable": "default_normalizers",
        "scrape_pages": true,
        "scrape_with_js": true,
        "url": "https://climate-energy.eea.europa.eu",
        "url_api_part": "api"
    },
    "config_fise": {
        "nlp_preprocessing": {
            "text": {
                "split_length": 500
            }
        },
        "normalize": {
            "blacklist": [],
            "location_rules": [
                {
                    "ct": [
                        "Topic page",
                        "Webpage"
                    ],
                    "path": "/topics/*"
                },
                {
                    "ct": [
                        "Webpage"
                    ],
                    "path": "/policy/*"
                },
                {
                    "ct": [
                        "Dashboard"
                    ],
                    "path": "/countries/regions/european-union"
                }
            ],
            "location_rules_fallback": "Webpage",
            "whitelist": [
                "Document",
                "News Item",
                "basic_data_factsheet",
                "european_union_factsheet",
                "country_biodiversity_factsheet",
                "country_bioeconomy_factsheet",
                "country_climate_factsheet",
                "country_vitality_factsheet"
            ]
        },
        "normalizers_variable": "default_normalizers",
        "scrape_pages": true,
        "scrape_with_js": true,
        "url": "https://forest.eea.europa.eu",
        "url_api_part": "api"
    },
    "config_industry": {
        "nlp_preprocessing": {
            "text": {
                "split_length": 500
            }
        },
        "normalize": {
            "blacklist": [],
            "location_rules": [
                {
                    "ct": [
                        "Data",
                        "Dashboard"
                    ],
                    "path": "/"
                },
                {
                    "ct": [
                        "Map (interactive)"
                    ],
                    "path": "/explore/explore-data-map/map"
                },
                {
                    "ct": [
                        "Dashboard"
                    ],
                    "path": "/analyse/*"
                },
                {
                    "ct": [
                        "Data"
                    ],
                    "path": "/download"
                }
            ],
            "location_rules_fallback": "Webpage",
            "whitelist": [
                "Document"
            ]
        },
        "normalizers_variable": "default_normalizers",
        "scrape_pages": true,
        "scrape_with_js": true,
        "url": "https://industry.eea.europa.eu",
        "url_api_part": "api"
    },
    "config_wise_freshwater": {
        "fix_items_url": {
            "with_api": "water.europa.eu/api/++api++/freshwater",
            "without_api": "water.europa.eu/freshwater"
        },
        "nlp_preprocessing": {
            "text": {
                "split_length": 500
            }
        },
        "normalize": {
            "blacklist": [],
            "location_rules": [
                {
                    "ct": [
                        "Topic page",
                        "Webpage"
                    ],
                    "path": "/freshwater/europe-freshwater/*"
                }
            ],
            "location_rules_fallback": "Webpage",
            "whitelist": [
                "Document",
                "File",
                "News Item",
                "visualization",
                "CompositePage",
                "dashboard",
                "dataset",
                "database",
                "briefing",
                "map_interactive",
                "country_profile"
            ]
        },
        "normalizers_variable": "default_normalizers",
        "scrape_pages": true,
        "scrape_with_js": true,
        "url": "https://water.europa.eu/freshwater",
        "url_api_part": ""
    },
    "config_wise_marine": {
        "concurrency": 1,
        "location_rules_fallback": "Webpage",
        "nlp_preprocessing": {
            "text": {
                "split_length": 500
            }
        },
        "normalize": {
            "blacklist": [],
            "location_rules": [
                {
                    "ct": [
                        "Webpage"
                    ],
                    "path": "/marine/policy-and-reporting/*"
                },
                {
                    "ct": [
                        "Topic page",
                        "Webpage"
                    ],
                    "path": "/marine/state-of-europe-seas/"
                },
                {
                    "ct": [
                        "Topic page",
                        "Webpage"
                    ],
                    "path": "/marine/state-of-europe-seas/*"
                },
                {
                    "ct": [
                        "Dashboard"
                    ],
                    "path": "/marine/state-of-europe-seas/marine-sectors-catalogue-of-measures"
                },
                {
                    "ct": [
                        "Dashboard"
                    ],
                    "path": "/marine/data-maps-and-tools/map-viewers-visualization-tools/dashboards-on-marine-features-under-other-policies/*"
                },
                {
                    "ct": [
                        "Dashboard"
                    ],
                    "path": "/marine/data-maps-and-tools/msfd-reporting-information-products/ges-assessment-dashboards/*"
                },
                {
                    "ct": [
                        "Country fact sheet",
                        "Dashboard"
                    ],
                    "path": "/marine/data-maps-and-tools/msfd-reporting-information-products/ges-assessment-dashboards/country-thematic-dashboards/"
                },
                {
                    "ct": [
                        "Data"
                    ],
                    "path": "/marine/data-maps-and-tools/msfd-reporting-information-products/msfd-reporting-data-explorer"
                },
                {
                    "ct": [
                        "Map (interactive)"
                    ],
                    "path": "/marine/data-maps-and-tools/map-viewers-visualization-tools/european-reference-maps"
                },
                {
                    "ct": [
                        "Country fact sheet",
                        "Dashboard"
                    ],
                    "path": "/marine/countries-and-regional-seas/country-profiles/*"
                }
            ],
            "whitelist": [
                "Document",
                "File",
                "country_factsheet"
            ]
        },
        "normalizers_variable": "default_normalizers",
        "scrape_pages": true,
        "scrape_with_js": true,
        "url": "https://water.europa.eu/marine",
        "url_api_part": ""
    },
